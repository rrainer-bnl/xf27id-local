# Set/Get alarm type
record(mbbo, "$(Sys)$(Dev)Type:Alrm1-Sel") 
{
	field(DTYP, "stream")
	field(DESC, "Set Alarm1 Type") 
	field(OUT,  "@cn7500.proto write_reg($(HEXADR),1020) $(PORT)")
	field(FLNK, "$(Sys)$(Dev)Type:Alrm1-Sts") 
	field(ZRST, "0:Disabled")
      field(ONST, "1:Dev Up-Lo")
      field(TWST, "2:Dev Up")
      field(THST, "3:Dev Lo")
      field(FRST, "4:Rev Up-Lo")
      field(FVST, "5:Abs Up-Lo")
      field(SXST, "6:Abs Up")
      field(SVST, "7:Abs Lo")
      field(EIST, "8:Std Up-Lo")
      field(NIST, "9:Std Up")
      field(TEST, "10:Std Lo")
	field(ELST, "11:Hys Up")
	field(TVST, "12:Hys Lo")
	field(TTST, "13:CT Alrm")
	field(ASG,  "VAWRITE")
}

