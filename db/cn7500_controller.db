	field(LNK3, "$(Sys)$(Dev)T:Lo-RB")
	field(LNK4, "$(Sys)$(Dev)Mode:Ctrl-Sts") 
	field(LNK5, "$(Sys)$(Dev)Mode:HC_Ctrl-Sts") 
	field(LNK6, "$(Sys)$(Dev)Type:Alrm1-Sts") 
