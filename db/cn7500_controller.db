## Database for Omega CN7500 temperature controller
## Substitutions:
## Pri		-	Primary system
## Sec		-	Secodary system, including serial number
## Dev		-	Device name
## HEXADR	-	Unit address
## PORT		-	IP or Serial port name

# Set if the controller is in use
record(bo, "$(Sys)$(Dev)Cmd:Use-Cmd")
{
	field(DESC, "Use Sel")
	field(ASG,  "VAWRITE")
	field(ZNAM, "Not use")
	field(ONAM, "In Use")
}

# Get process value
record(ai, "$(Sys)$(Dev)T-I")
{
	field(DTYP, "stream")
	field(DESC, "Process Value")
	field(INP,  "@cn7500.proto read_reg($(HEXADR),1000) $(PORT)")
	field(DISV, "0")
	field(SDIS, "$(Sys)$(Dev)Cmd:Use-Cmd")
	field(LINR, "SLOPE")
	field(ESLO, "0.1")
	field(PREC, "1")
	field(EGU,  "Celsius")
	field(SCAN, "1 second")
	field(LSV,  "INVALID")
	field(HSV,  "MINOR")
	field(HHSV, "MAJOR")
	field(LOW,  "0")
	field(HIGH, "160")
	field(HIHI, "210")
}

## Scan records
# Calc the sequence for scan
record(calcout, "$(Sys)$(Dev)DB:Scan-Calc_") 
{
	field(DESC, "Scan Calc")
	field(DISV, "0")
	field(SDIS, "$(Sys)$(Dev)Cmd:Use-Cmd")
	field(SCAN, "5 second")
	field(CALC, "(A<B)?(A+C):D")
	field(INPA, "$(Sys)$(Dev)DB:Scan-Calc_")
	field(INPB, "7")
	field(INPC, "1")
	field(INPD, "1")
	field(OUT,  "$(Sys)$(Dev)DB:Scan-FOut_.SELN")
	field(FLNK, "$(Sys)$(Dev)DB:Scan-FOut_")
}

record(fanout, "$(Sys)$(Dev)DB:Scan-FOut_")
{
	field(DESC, "Scan Fanout1")
	field(SELM, "Specified")
	field(LNK1, "$(Sys)$(Dev)DB:Scan1-FOut_")
	field(LNK2, "$(Sys)$(Dev)DB:Scan2-FOut_")
	field(LNK3, "$(Sys)$(Dev)DB:Scan3-FOut_")
	field(LNK4, "$(Sys)$(Dev)DB:Scan4-FOut_")
	field(LNK5, "$(Sys)$(Dev)DB:Scan5-FOut_")
	field(LNK6, "$(Sys)$(Dev)DB:Scan6-FOut_")
}

