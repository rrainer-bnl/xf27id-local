## Database for Hiden RC Interface RGA

## Set up parameters for MID mode
# Set particular mass 1
record(longout, "$(Sys)$(Dev)Mass:MID1-SP")
{
	field(DESC, "Set Start Mass")
	field(LOPR, "1")
	field(HOPR, "100")
	field(VAL,  "1")
        field(ASG,  "VAWRITE")
        info(autosaveFields_pass0, "VAL")
}

# Set detector type
record(mbbo, "$(Sys)$(Dev)Type:MID1Detr-Sel")
{
	field(DESC, "Set Detector Type")
	field(ZRST, "Faraday")
	field(ONST, "SEM")
	field(VAL,  "1")
        field(ASG,  "VAWRITE")
        info(autosaveFields_pass0, "VAL")
}

#  Check autoranging
record(bo, "$(Sys)$(Dev)P:MID1AutoRng-Sel")
{
	field(DESC, "Set Autoranging")
	field(ZNAM, "NO")
	field(ONAM, "YES")
	field(VAL,  "1")
	field(FLNK, "$(Sys)$(Dev)DB:MID1Rng-FOut_")
        field(ASG,  "VAWRITE")
        info(autosaveFields_pass0, "VAL")
}

# Download data to controller
record(fanout, "$(Sys)$(Dev)DB:MID1Rng-FOut_")
{
	field(DESC, "MID1 Range Fanout")
	field(SDIS, "$(Sys)$(Dev)P:MID1AutoRng-Sel")
	field(DISV, "0")
	field(LNK1, "$(Sys)$(Dev)DB:MID1F_Rng-Seq_")
	field(LNK2, "$(Sys)$(Dev)DB:MID1SEM_Rng-Seq_")
}

# Set the pressure range limit for autoranging
record(seq, "$(Sys)$(Dev)DB:MID1F_Rng-Seq_")
{
	field(DESC, "Faraday Range")
	field(SELM, "All")
	field(SDIS, "$(Sys)$(Dev)Type:MID1Detr-Sel")
	field(DISV, "1")
	field(DOL1, "$(Sys)$(Dev)P:F_RngMin-RB PP")
	field(DOL2, "$(Sys)$(Dev)P:F_RngMax-RB PP")
	field(DOL3, "$(Sys)$(Dev)P:F_RngMax-RB PP")
	field(LNK1, "$(Sys)$(Dev)P:MID1Lo-SP PP")
	field(LNK2, "$(Sys)$(Dev)P:MID1Hi-SP PP")
	field(LNK3, "$(Sys)$(Dev)P:MID1Curt-SP PP")
}

record(seq, "$(Sys)$(Dev)DB:MID1SEM_Rng-Seq_")
{
	field(DESC, "SEM Range")
	field(SELM, "All")
	field(SDIS, "$(Sys)$(Dev)Type:MID1Detr-Sel")
	field(DISV, "0")
	field(DOL1, "$(Sys)$(Dev)P:SEM_RngMin-RB PP")
	field(DOL2, "$(Sys)$(Dev)P:SEM_RngMax-RB PP")
	field(DOL3, "$(Sys)$(Dev)P:SEM_RngMax-RB PP")
	field(LNK1, "$(Sys)$(Dev)P:MID1Lo-SP PP")
	field(LNK2, "$(Sys)$(Dev)P:MID1Hi-SP PP")
	field(LNK3, "$(Sys)$(Dev)P:MID1Curt-SP PP")
}
# Set the low pressure range limit for autoranging
record(longout, "$(Sys)$(Dev)P:MID1Lo-SP")
{
	field(DESC, "Set Low Pr")
	field(LOPR, "-13")
	field(HOPR, "-10")
	field(VAL,  "-13")
        field(ASG,  "VAWRITE")
        info(autosaveFields_pass0, "VAL")
}

# Set the high pressure limit for autoranging
