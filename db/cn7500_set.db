## Database for Omega CN7500 temperature controller
## Substitutions:
## Pri		-	Primary system
## Sec		-	Secodary system

# Set/Get pattern temperature set point
record(ao, "$(P){}T-SP")
{
        field(DESC, "Set SV SP")
        field(DRVH, "999.9")
        field(DRVL, "-99.9")
        field(LINR, "SLOPE")
        field(ESLO, "0.1")
        field(VAL,  "50")
        field(EGU,  "Celsius")
        field(ASG,  "VAWRITE")
}

