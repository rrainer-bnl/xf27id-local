# Set/Get pattern execution time
# Step 0
record(longout, "$(P){}Time:Step0HH-SP") 
{
	field(DESC, "Set Step0 Hour")
	field(DRVH, "15")
	field(DRVL, "0")
	field(VAL,  "0")
	field(EGU,  "Hour")
	field(ASG,  "VAWRITE")
	field(FLNK, "$(P){}Time:Step0-Calc_") 
}

record(longout, "$(P){}Time:Step0MM-SP") 
{
	field(DESC, "Set Step0 Minute")
	field(DRVH, "60")
	field(DRVL, "0")
	field(VAL,  "30")
	field(EGU,  "Minute")
	field(ASG,  "VAWRITE")
	field(FLNK, "$(P){}Time:Step0-Calc_") 
}

record(calcout, "$(P){}Time:Step0-Calc_") 
{
	field(DESC, "Step0 K Calc")
	field(INPA, "$(P){}Time:Step0HH-SP")
	field(INPB, "$(P){}Time:Step0MM-SP")
	field(CALC, "(A*60+B)")
	field(OUT,  "$(P){}Time:Step0-SP PP") 
}

record(longout, "$(P){}Time:Step0-SP") 
{
	field(DESC, "Set Step0 Time")
	field(DRVH, "900")
	field(DRVL, "0")
	field(VAL,  "30")
	field(EGU,  "Minute")
	field(ASG,  "VAWRITE")
}

