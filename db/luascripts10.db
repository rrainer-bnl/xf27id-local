record(bo, "$(P)$(R)userScriptEnable")
{
	field(DESC, "userScript enable")
	field(ZNAM, "Disable")
	field(ONAM, "Enable")
	field(UDF, "0")
	field(STAT, "NO_ALARM")
	field(SEVR, "NO_ALARM")
}

record(bo, "$(P)$(R)EnableUserScripts")
{
	field(OMSL, "closed_loop")
	field(DOL, "1")
	field(OUT, "$(P)$(R)userScriptEnable")
}

record(bo, "$(P)$(R)DisableUserScripts")
{
	field(OMSL, "closed_loop")
	field(DOL, "0")
	field(OUT, "$(P)$(R)userScriptEnable")
}

record(luascript, "$(P)$(R)userScript0")
{
	field(DESC, "userScript0")
	field(DISV, "0")
	field(SDIS, "$(P)$(R)userScriptEnable")
	field(PREC, "5")
}

