#define autoincr p90
#define stop p91
#define currentPos p92
#define homeOffset m$(axis)73 + (i$(axis)26 / 16)
#define cmpA M$(eaxis)08
#define cmpB M$(eaxis)09

open PLC0 clear

currentPos=m$(axis)62/(I$(axis)08*32) + homeOffset

if (currentPos !< cmpA)
and (cmpA + autoincr < stop)
	cmpA = cmpA + autoincr
endif

if (currentPos !< cmpB)
and (cmpB + autoincr < stop)
	cmpB = cmpB + autoincr
endif

close
